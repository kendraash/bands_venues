<h1 >Venues And Bands</h1>
    <h2>Venues:</h2>
    <ul class="list-unstyled">
      <% @venues.each do |venue| %>
      <li>
        <a href="/venues/<%=venue.id%>"><%=venue.name%> | <%=venue.city%>, <%=venue.state%></a>
      </li>
      <%end%>
    </ul>

<a href="/venues/new" class="btn btn-primary">Add a new Venue</a>


<a href="/bands" class="btn btn-warning">View all Bands</a>
